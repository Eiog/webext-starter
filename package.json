{
  "name": "vue-demo",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "dev:web": "vite --mode web",
    "dev:firefox": "TARGET=firefox vite build --watch --mode development",
    "build": "vue-tsc --noEmit && vite build ",
    "build:firefox": "vue-tsc --noEmit && TARGET=firefox vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "update-deps": "taze -w && pnpm i",
    "type": "vue-tsc --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@unocss/reset": "^0.51.13",
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "colord": "^2.9.3",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@eiog/eslint-config": "^0.0.1",
    "@iconify-json/ri": "^1.1.8",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/webextension-polyfill": "^0.10.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/ui": "^0.31.0",
    "@vue/compiler-sfc": "^3.3.2",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.40.0",
    "jsdom": "^22.0.0",
    "less": "^4.1.3",
    "naive-ui": "^2.34.3",
    "taze": "^0.10.1",
    "typescript": "^5.0.4",
    "unocss": "^0.51.13",
    "unocss-preset-extra": "^0.5.2",
    "unocss-preset-scrollbar": "^0.2.1",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-define-options": "^1.3.5",
    "vfonts": "^0.0.3",
    "vite": "^4.3.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-web-extension": "^3.0.7",
    "vitest": "^0.31.0",
    "vue": "^3.3.2",
    "vue-tsc": "^1.6.5",
    "web-ext": "^7.6.2",
    "webext-bridge": "^6.0.1",
    "webextension-polyfill": "^0.10.0"
  }
